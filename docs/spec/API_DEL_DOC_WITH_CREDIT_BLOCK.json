    "/CheckCreditBlock": {
      "post": {
        "summary": "Checks the credit status of a specific delivery document.",
        "description": "Checks credit status of a delivery document. Consumers must pass the delivery document number and SD document category.",
        "tags": [
          "Service Operations"
        ],
        "parameters": [
          {
            "name": "DeliveryDocument",
            "in": "query",
            "required": true,
            "description": "Value needs to be enclosed in single quotes",
            "schema": {
              "type": "string",
              "pattern": "^'[^']*(''[^']*)*'$"
            }
          },
          {
            "name": "SDDocumentCategory",
            "in": "query",
            "required": true,
            "description": "Value needs to be enclosed in single quotes",
            "schema": {
              "type": "string",
              "pattern": "^'[^']*(''[^']*)*'$"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "title": "ReturnMessage",
                  "type": "object",
                  "properties": {
                    "d": {
                      "type": "object",
                      "properties": {
                        "CheckCreditBlock": {
                          "$ref": "#/components/schemas/API_DEL_DOC_WITH_CREDIT_BLOCK.ReturnMessage"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "4XX": {
            "$ref": "#/components/responses/error"
          }
        }
      }
    },
